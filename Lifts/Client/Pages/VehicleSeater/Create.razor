@page "/VehicleSeaters/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New Vehicle Seater</h3>
<hr />
<br />

<FormComponent ButtonText="Create Seater" vehicleSeater="vehicleSeater" OnValidSubmit="@CreateVehicleSeater" />

@code { VehicleSeater vehicleSeater = new VehicleSeater();

    private async Task CreateVehicleSeater()
    {
        await _client.PostAsJsonAsync(Endpoints.VehicleSeatersEndpoint, vehicleSeater);
        _navManager.NavigateTo("/vehicleSeater/");
    } }