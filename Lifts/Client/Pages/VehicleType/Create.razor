@page "/VehicleTypes/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New Vehicle Type</h3>
<hr />
<br />

<FormComponent ButtonText="Create Type" vehicleType="vehicleType" OnValidSubmit="@CreateVehicleType" />

@code { VehicleType vehicleType = new VehicleType();

    private async Task CreateVehicleType()
    {
        await _client.PostAsJsonAsync(Endpoints.VehicleTypesEndpoint, vehicleType);
        _navManager.NavigateTo("/vehicleType/");
    } }